{
    "name": "sakai-react",
    "version": "10.1.0-SNAPSHOT",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "format": "prettier --write \"{components,app,service,api-generator}/**/*.{js,ts,tsx,d.ts}\"",
        "lint": "next lint",
        "dev:local": "env-cmd -f ./.env.local next dev",
        "dev:Dev": "env-cmd -f ./.env.development next dev",
        "build:local": "env-cmd -f ./.env.local next build",
        "build:Dev": "env-cmd -f ./.env.development next build",
        "start:local": "env-cmd -f ./.env.local next start",
        "start:Dev": "env-cmd -f ./.env.development next start",
        "serve": "serve -s build"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^2.0.1",
        "@types/date-fns": "^2.6.0",
        "@types/node": "20.3.1",
        "@types/react": "18.2.12",
        "@types/react-dom": "18.2.5",
        "axios": "^1.6.2",
        "chart.js": "4.2.1",
        "date-fns": "^2.30.0",
        "next": "13.4.8",
        "next-auth": "^4.24.5",
        "primeflex": "^3.3.1",
        "primeicons": "^6.0.1",
        "primereact": "^9.6.2",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-redux": "^9.0.4",
        "react-router-dom": "^6.21.0",
        "typescript": "5.1.3"
    },
    "devDependencies": {
        "env-cmd": "^10.1.0",
        "eslint": "8.43.0",
        "eslint-config-next": "13.4.6",
        "prettier": "^2.8.8",
        "sass": "^1.63.4"
    }
}
